{
    "name": "CaptureLog",
    "short_name": "CaptureLog",
    "description": "Capture Log using Chrome Extension V3",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "debugger"
    ]
}